{
    "_version": "1.32.0",
    "sap.app": {
        "id": "com.knpl.tssportal",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "dataSources": {
            "mainService": {
                "uri": "/odata/v4/DefectService/",
                "type": "OData",
                "settings": {
                    "annotations": [
                        "annotation"
                    ],
                    "odataVersion": "4.0",
                    "localUri": "localService/metadata.xml"
                }
            },
            "annotation": {
                "type": "ODataAnnotation",
                "uri": "annotations/annotation.xml",
                "settings": {
                    "localUri": "annotations/annotation.xml"
                }
            }
        },
        "offline": false,
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "ui5template.fiorielements.v4.lrop",
            "version": "1.0.0"
        },
        "crossNavigation": {
            "inbounds": {
                "com-knpl-tssportal-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "TSS",
                    "action": "Manage",
                    "title": "{{flpTitle}}",
                    "subTitle": "{{flpSubtitle}}",
                    "icon": "sap-icon://manager-insight"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "resources": {
            "js": [],
            "css": []
        },
        "dependencies": {
            "minUI5Version": "1.98.0",
            "libs": {
                "sap.ui.core": {},
                "sap.fe.templates": {},
                "sap.f": {}
            }
        },
        "models": {
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            }
        },
        "routing": {
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "DefectDetailsList",
                    "target": [
                        "DefectDetailsList"
                    ]
                },
                {
                    "pattern": "DefectDetails({key}):?query:",
                    "name": "DefectDetailsObjectPage",
                    "target": [
                        "DefectDetailsList",
                        "DefectDetailsObjectPage"
                    ]
                },
                {
                    "pattern": "DefectDetails({key})/versions({key2}):?query:",
                    "name": "DefectDetailVersionsObjectPage_versions",
                    "target": [
                        "DefectDetailsList",
                        "DefectDetailsObjectPage",
                        "DefectDetailVersionsObjectPage_versions"
                    ]
                }
            ],
            "targets": {
                "DefectDetailsList": {
                    "type": "Component",
                    "id": "DefectDetailsList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "entitySet": "DefectDetails",
                            "variantManagement": "None",
                            "navigation": {
                                "DefectDetails": {
                                    "detail": {
                                        "route": "DefectDetailsObjectPage"
                                    }
                                }
                            },
                            "content": {
                                "header": {
                                    "visible": true,
                                    "anchorBarVisible": true,
                                    "actions": {
                                        "CustomActionOPHeader": {
                                            "press": "com.knpl.tssportal.ext.CustomController.onRaisePress",
                                            "visible": true,
                                            "text": "Raise a Request"
                                        }
                                    }
                                }
                            },
                            "initialLoad": "Enabled",
                            "controlConfiguration": {
                                "@com.sap.vocabularies.UI.v1.LineItem": {
                                    "actions": {
                                        "button": {
                                            "text": "Download"
                                           
                                        }
                                    },
                                    "columns": {
                                        "DataField::title": {
                                            "width": "100%"
                                        },
                                        "DataField::defectType::name": {
                                            "width": "100%"
                                        },
                                        "DataField::product::name": {
                                            "width": "100%"
                                        },
                                        "DataField::product::code": {
                                            "width": "100%"
                                        },
                                        "DataField::product::vehicleType::name": {
                                            "width": "100%"
                                        },
                                        "DataField::product::mainCustomer::name": {
                                            "width": "100%"
                                        },
                                        "DataField::vehicleType::name": {
                                            "width": "100%"
                                        },
                                        "DataField::mainCustomer::name": {
                                            "width": "100%"
                                        }
                                    },
                                    "tableSettings": {
                                        "enableExport": false
                                    }
                                }
                            }
                        }
                    },
                    "controlAggregation": "beginColumnPages",
                    "contextPattern": ""
                },
                "DefectDetailsObjectPage": {
                    "type": "Component",
                    "id": "DefectDetailsObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "editableHeaderContent": false,
                            "entitySet": "DefectDetails",
                            "navigation": {
                                "versions": {
                                    "detail": {
                                        "route": "DefectDetailVersionsObjectPage_versions"
                                    }
                                }
                            },
                            "controlConfiguration": {
                                "versions/@com.sap.vocabularies.UI.v1.LineItem#i18nversion1": {
                                    "columns": {
                                        "DataField::version": {
                                            "width": "100%"
                                        },
                                        "DataField::createdAt": {
                                            "width": "100%"
                                        },
                                        "DataField::createdBy": {
                                            "width": "100%"
                                        }
                                    },
                                    "tableSettings": {
                                        "enableExport": false,
                                        "personalization": {
                                            "filter": false
                                        }
                                    }
                                }
                            },
                            "sectionLayout": "Tabs"
                        }
                    },
                    "controlAggregation": "midColumnPages",
                    "contextPattern": "/DefectDetails({key})"
                },
                "DefectDetailVersionsObjectPage_versions": {
                    "type": "Component",
                    "id": "DefectDetailVersionsObjectPage_versions",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "entitySet": "DefectDetailVersions",
                            "navigation": {}
                        }
                    },
                    "controlAggregation": "endColumnPages",
                    "contextPattern": "/DefectDetails({key})/versions({key2})"
                },
                "": {
                    "options": {
                        "settings": {
                            "controlConfiguration": {
                                "@com.sap.vocabularies.UI.v1.LineItem": {
                                    "actions": {
                                        "idButton": {
                                            "id": "idButtonButton",
                                            "text": "Download",
                                            "press": "com.knpl.tssportal.custom.ListReportExtController.onCustomAction",
                                            "requiresSelection": false
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "config": {
                "flexibleColumnLayout": {
                    "defaultTwoColumnLayoutType": "TwoColumnsMidExpanded",
                    "defaultThreeColumnLayoutType": "ThreeColumnsEndExpanded"
                },
                "routerClass": "sap.f.routing.Router"
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "rootView": {
            "viewName": "sap.fe.templates.RootContainer.view.Fcl",
            "type": "XML",
            "async": true,
            "id": "appRootView"
        }
    },
    "sap.platform.abap": {
        "_version": "1.1.0",
        "uri": ""
    },
    "sap.platform.hcp": {
        "_version": "1.1.0",
        "uri": ""
    },
    "sap.fiori": {
        "_version": "1.1.0",
        "registrationIds": [],
        "archeType": "transactional"
    },
    "sap.cloud": {
        "public": true,
        "service": "knpl.tss"
    }
}