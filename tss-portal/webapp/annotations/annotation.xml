<edmx:Edmx xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx" Version="4.0">
    <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/UI.xml">
        <edmx:Include Namespace="com.sap.vocabularies.UI.v1" Alias="UI" />
    </edmx:Reference>
    <edmx:Reference Uri="/odata/v4/DefectService/$metadata">
        <edmx:Include Namespace="DefectService" />
    </edmx:Reference>
    <edmx:DataServices>
        <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="com.knpl">
            <Annotations Target="DefectService.DefectDetailVersions">
                <Annotation Term="UI.LineItem">
                    <Collection>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="version" />
                            <PropertyValue Property="Label" String="{@i18n>version}" />
                        </Record>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="createdAt" />
                            <PropertyValue Property="Label" String="{@i18n>createdAt}" />
                        </Record>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="createdBy" />
                            <PropertyValue Property="Label" String="{@i18n>createdBy}" />
                        </Record>
                    </Collection>
                </Annotation>
                <Annotation Term="UI.LineItem" Qualifier="i18nversion1">
                    <Collection>
                        
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="version"/>
                            <PropertyValue Property="Label" String="{@i18n>version}"/>
                        </Record>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="createdAt"/>
                        </Record>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="createdBy"/>
                        </Record></Collection>
                </Annotation>
            </Annotations>
            <Annotations Target="DefectService.DefectDetails">
                <Annotation Term="UI.LineItem">
                    <Collection>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="title"/>
                            <PropertyValue Property="Label" String="{@i18n>title}"/>
                        </Record>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="defectType/name"/>
                            <PropertyValue Property="Label" String="{@i18n>defectType}"/>
                        </Record>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="product/name"/>
                            <PropertyValue Property="Label" String="{@i18n>productName}"/>
                        </Record>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="vehicleType/name"/>
                            <PropertyValue Property="Label" String="{@i18n>vehicleType}"/>
                        </Record>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="mainCustomer/name"/>
                            <PropertyValue Property="Label" String="{@i18n>mainCustomer}"/>
                        </Record>
                    </Collection>
                </Annotation>
                <Annotation Term="UI.Facets">
                    <Collection>
                        <Record Type="UI.CollectionFacet">
                            <PropertyValue Property="Label" String="{@i18n>details}"/>
                            <PropertyValue Property="Facets">
                                <Collection>
                                    <Record Type="UI.ReferenceFacet">
                                        <PropertyValue Property="Target" AnnotationPath="@UI.FieldGroup#ProductDetails"/>
                                        <PropertyValue Property="Label" String="Product Details"/>
                                    </Record>
                                    <Record Type="UI.ReferenceFacet">
                                        <PropertyValue Property="Target" AnnotationPath="@UI.FieldGroup#Main"/>
                                        <PropertyValue Property="Label" String="Basic Details"/>
                                    </Record>
                                    <Record Type="UI.ReferenceFacet">
                                        <PropertyValue Property="Target" AnnotationPath="@UI.FieldGroup#Main2"/>
                                        <PropertyValue Property="Label" String="Defect Details"/>
                                    </Record>
                                    <Record Type="UI.ReferenceFacet">
                                        <PropertyValue Property="Target" AnnotationPath="@UI.FieldGroup#Main3"/>
                                        <PropertyValue Property="Label" String="Paint Details"/>
                                    </Record>
                                </Collection>
                            </PropertyValue>
                        </Record>
                        <Record Type="UI.ReferenceFacet">
                            <PropertyValue Property="Label" String="{@i18n>version1}"/>
                            <PropertyValue Property="ID" String="i18nversion1"/>
                            <PropertyValue Property="Target" AnnotationPath="versions/@UI.LineItem#i18nversion1"/>
                        </Record>
                    </Collection>
                </Annotation>
            </Annotations>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>